# MTProxy Docker Configuration
# Copy this file to .env and fill in the values

# OCIR Registry (get namespace from OCI Console -> Tenancy Details)
IMAGE_REGISTRY=fra.ocir.io/YOUR_NAMESPACE

# Primary Proxy
MTPROXY_PORT=443
MTPROXY_SECRET=your_16_byte_hex_secret_here
MTPROXY_DOMAIN=cdn.jsdelivr.net

# Secondary Proxy
MTPROXY_SECONDARY_PORT=8443
MTPROXY_SECONDARY_SECRET=your_secondary_16_byte_hex_secret_here
MTPROXY_SECONDARY_DOMAIN=wildberries.ru
